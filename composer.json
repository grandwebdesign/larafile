{
    "name": "grandwebdesign/larafile",
    "description": "File handler for Laravel, Upload and Remove files",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Grandwebdesign\\Larafile\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dominik Jaross",
            "email": "dominikjaross@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/http": "^11.0",
        "illuminate/support": "^11.0",
        "tinify/tinify": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grandwebdesign\\Larafile\\LarafileProvider"
            ]
        }
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
